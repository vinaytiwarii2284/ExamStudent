@*<link href="~/Content/style.css" rel="stylesheet" />*@
@using (Html.BeginForm("Claim", "QRClaim", FormMethod.Post, new { @class = "", role = "form", id = "inner11", autocomplete = "off" }))
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)
    <div class="claim-page">
        @*<div class="row">*@
        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12 mt-20">
            <h4 class="text-left">@Digiphoto.iMix.ClaimPortal.Common.Resources.en_US.ClaimQRCodePartialViewClaimCodeText</h4>
            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                @Html.TextBoxFor(model => model.IdentificationCodeFirst, new { @class = "form-control small_TextBox", maxlength = 4, @onkeyup = "Autoindexing()", htmlAttributes = new { tabindex = 1, @width = "50px" } })
            </div>

            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                @Html.TextBoxFor(model => model.IdentificationCodeSecond, new { @class = "form-control claimcodeinput small_TextBox", maxlength = 4 })
            </div>
            <div class="no-padding" style="margin-left: 16px;">
                @Html.ValidationMessageFor(model => model.IdentificationCodeSecond)
                <span class="field-validation-error" id="ClaimErrorMsg" style="color: #F00; float: left;">
                    <div style="float:left;" id="ClaimError">@TempData["ClaimError"]</div>
                </span>
            </div>
            <div class="no-padding-left" style="text-align:left">

                @*<a class="gobtn" href="myphotos.html"><i class="fa fa-arrow-circle-o-right" aria-hidden="true"></i> Continue</a>*@
                <button type="submit" class="gobtn" href="myphotos.html"><i class="fa fa-arrow-circle-o-right" aria-hidden="true"></i> @Digiphoto.iMix.ClaimPortal.Common.Resources.en_US.LoginPartialViewGoTittleText</button>
            </div>
        </div>
        <div class="ht10"></div>
        <div class="clearfix"></div>

        @*<div class="clearfix"></div>*@
        @*</div>*@
    </div>
}

@* Mobile App Modal *@
<div class="modal fade" id="appStoreModal" tabindex="-1" role="dialog" data-backdrop="static" aria-labelledby="appStoreModal" aria-hidden="true" style="top:35%">
    <div class="modal-dialog-app-download">
        <div class="modal-content custom_modal">
            <div class="modal-header smaller modalHeaderBG text-center">
                <h4>Download Mobile App</h4>
                <div class="clearfix"></div>
                <div class="col-lg-6 col-lg-offset-6 content-center">
                    <a href="#" id="appStoreLink" class="installappbtn"> Ok </a>
                    <input type="button" value="Cancel" class="cancelappbtn" data-dismiss="modal" />


                    @*<a href="#" id="appStoreLink"><input type="button" value="Ok" class="btn btn-default btn-popup" /></a>
                    <input type="button" value="Cancel" class="btn btn-default btn-popup" data-dismiss="modal" />*@
                </div>
            </div>
        </div>
    </div>
</div>

