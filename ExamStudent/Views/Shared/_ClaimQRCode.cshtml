    $(document).ready(function () {
        var codeFirst = $("#IdentificationCodeFirst").val()
        var codeSecond = $("#IdentificationCodeSecond").val()

        $("#IdentificationCodeFirst").on('change keyup paste', function () {
            if ($("span.ValidationErrorMsg span").hasClass("field-validation-error")) {
                $("#ClaimErrorMsg").text('');
            }
        });
        $("#IdentificationCodeSecond").on('change keyup paste', function () {
            if ($("span.ValidationErrorMsg span").hasClass("field-validation-error")) {
                $("#ClaimErrorMsg").text('');
            }
        });
        $("#btnGo").on('click', function () {
            if (codeFirst == "") {
                $("#ClaimErrorMsg").text('');
            }
            if (codeSecond == "") {
                $("#ClaimErrorMsg").text('');
            }
        });



        @{
            if (Model != null)
            {
                if (Model.ScanHit == true)
            {
                @:GetDeviceInfo();
            }
            }
        }

    });

    function GetDeviceInfo() {
        var isDeviceOS = {
            Android: function () {
                return navigator.userAgent.match(/Android/i);
            },
            BlackBerry: function () {
                return navigator.userAgent.match(/BlackBerry/i);
            },
            iOS: function () {
                return navigator.userAgent.match(/iPhone|iPad|iPod/i);
            },
            Opera: function () {
                return navigator.userAgent.match(/Opera Mini/i);
            },
            Windows: function () {
                return navigator.userAgent.match(/IEMobile/i) || navigator.userAgent.match(/WPDesktop/i);
            },
            any: function () {
                return (isMobile.Android() || isMobile.BlackBerry() || isMobile.iOS() || isMobile.Opera() || isMobile.Windows());
            }
        };

        var isMobileOrTablet = mobileAndTabletcheck();
        if (isMobileOrTablet) {
            if (isDeviceOS.iOS()) {
                $('#appStoreLink').attr('href', '@System.Configuration.ConfigurationManager.AppSettings["iOSApp"]');
                $('#appStoreModal').modal('show');
            }
            if (isDeviceOS.Android()) {
                $('#appStoreLink').attr('href', '@System.Configuration.ConfigurationManager.AppSettings["AndroidApp"]');
                $('#appStoreModal').modal('show');
            }
            if (isDeviceOS.Windows()) {
                // For Windows Mobiles
            }
        }
        else {
            //alert('else');
        }
    }

