<h2 class="display-4 text-center">Gallery</h2>
<br />
<br />
<div class="container">

    <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
        <div class="carousel-inner">
            <div class="carousel-item active">

                <img class="d-block w-100" src="~/images/32-education_facebook.jpg" alt="First slide">
            </div>


        </div>
        <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>

    <div class="row">
        <div class="col-md-4">
            <img  />
            <img src="~/images/WhatsApp Image 2020-11-28 at 2.27.20 PM.jpeg" alt="..." class="img-thumbnail">
        </div>
        <div class="col-md-4">
            <img  />
            <img src="~/images/WhatsApp Image 2020-11-28 at 2.27.21 PM.jpeg" alt="..." class="img-thumbnail">
        </div>
        <div class="col-md-4">
            <img  />
            <img src="~/images/WhatsApp Image 2020-11-28 at 2.27.22 PM.jpeg" alt="..." class="img-thumbnail">
        </div>
    </div>




</div>


