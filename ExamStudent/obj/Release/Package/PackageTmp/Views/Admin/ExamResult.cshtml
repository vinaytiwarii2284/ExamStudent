
@{
    ViewBag.Title = "ExamResult";
    Layout = "~/Views/Shared/_Layout2.cshtml";
}

<h2 class="display-4"> Exam Result</h2>

<div class="container">

    <form method="post">
        <div class="form-group">
            <label>Exam Date Time</label>
            <input type="datetime-local" id="birthdaytime" name="ExamDateTime" class="form-control  ">
        </div>

        <div class="form-group">
            <label>Percentange</label>
            <input type="number" name="Percentange" class="form-control col-md-5">
        </div>


        <div class="form-group">
            <label>Answer Count</label>
            <input type="number" name="AnswerCount" class="form-control col-md-5">
        </div>

        <div class="form-group">
            <label>Correct Answer</label>
            <input type="text" name="AnswerIDs" class="form-control col-md-5">
        </div>

        <button class="btn btn-outline-primary">Save</button>
    </form>  
</div>

